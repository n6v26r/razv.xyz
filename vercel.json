{
  "routes": [
    {
      "src": "/fonts/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/fonts/$1"
    },
    {
      "src": "/(.*)/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1/images/$2"
    },
    {
      "src": "/_app/immutable/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/_app/immutable/$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}
